{
  "name": "jsx-htmx",
  "version": "0.0.1",
  "description": "TypeSafe HTML templates using TypeScript. No need to learn a template library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BetterCorp/jsx-htmx.git"
  },
  "scripts": {
    "build": "tsc"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./lib/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "require": "./lib/jsx-runtime.js",
      "types": "./lib/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "require": "./lib/jsx-dev-runtime.js",
      "types": "./lib/jsx-dev-runtime.d.ts"
    },
    "./lib/jsx-runtime": {
      "require": "./lib/jsx-runtime.js",
      "types": "./lib/jsx-runtime.d.ts"
    },
    "./lib/jsx-dev-runtime": {
      "require": "./lib/jsx-dev-runtime.js",
      "types": "./lib/jsx-dev-runtime.d.ts"
    }
  },
  "keywords": [
    "typescript",
    "html",
    "htmx",
    "typesafe",
    "template"
  ],
  "files": [
    "lib",
    "README.md",
    "src/index.d.ts"
  ],
  "author": "Mitchell R <github@mrincworld.com>",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@stryker-mutator/core": "^5.2.2",
    "@stryker-mutator/mocha-runner": "^5.2.2",
    "@stryker-mutator/typescript": "^4.0.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.7",
    "chai": "^4.3.4",
    "conventional-changelog-cli": "^2.1.1",
    "html-differ": "^1.4.0",
    "mocha": "^9.0.3",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "tslint": "^5.20.1",
    "typescript": "^4.3.5"
  }
}